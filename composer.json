{
    "name": "groundstack/gs-monitor-observer",
    "type": "typo3-cms-extension",
    "description": "GroundStack - Monitor Observer",
    "authors": [
        {
            "name": "Christian Hackl",
            "email": "info@groundstack.de",
            "homepage": "http://www.groundstack.de",
            "role": "Developer"
        }
    ],
    "version": "0.1.0",
    "license": ["MIT"],
    "keywords": ["TYPO3 CMS", "extension"],
    "replace": {
        "hauer-heinrich/gs_monitor_observer": "self.version",
        "typo3-ter/gs-monitor-observer": "self.version"
    },
    "require": {
        "typo3/cms-core": "^9.5.0 || ^9.9.99",
        "typo3/cms-fluid": "^9.5.0 || ^9.9.99",
        "typo3/cms-frontend": "^9.5.0 || ^9.9.99",
        "typo3/cms-fluid-styled-content": "^9.5.0 || ^9.9.99"
    },
    "autoload": {
        "psr-4": {
            "GroundStack\\gs_monitor_observer\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "gs_monitor_observer"
        }
    }
}
